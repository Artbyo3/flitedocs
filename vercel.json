{
  "version": 2,
  "buildCommand": "python -m pip install -r requirements.txt",
  "outputDirectory": ".",
  "functions": {
    "run.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/app/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/run.py"
    }
  ]
}
